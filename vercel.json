{
    "version": 2,
    "builds": [
      {
        "src": "src/api/**/*.ts",
        "use": "@vercel/node"
      },
      {
        "src": "dist/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/src/api/$1.ts"
      },
      {
        "src": "/(.*)",
        "dest": "/dist/$1"
      }
    ]
  }